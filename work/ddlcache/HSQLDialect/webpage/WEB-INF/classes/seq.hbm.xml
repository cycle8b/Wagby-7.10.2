      package="jp.jasminesoft.jfc.app.seq">

    <import class="Seq"/>

    <class name="Seq" table="&quot;seq&quot;">
        <id name="name_">
            <column name="&quot;name&quot;"/>
        </id>

        <property name="number_">
            <column name="&quot;number&quot;"/>
        </property>

    </class>

</hibernate-mapping>
